@model List<Employee>
@{
    ViewData["Title"] = "Employees";
}

<h1>Employees</h1>

<table>
    <tr>
        <th>First Name</th>
        <th>Age</th>
        <th>Salary</th>
    </tr>
    @foreach (Employee employee in Model)
    {
<tr>
    <td>@employee.Id</td>
    <td>@employee.FirstName</td>
    <td>@employee.Age</td>
    <td>@employee.Salary</td>

</tr>
    }
</table>

<form action="../Home/RetirementInfo" method="post">
    Employee By Id: <input type="number" name="Id" value="" />
    <input type="submit" name="name" value="Submit" />


</form>